<?xml version="1.0" encoding="ASCII"?>
<xmi:XMI xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:playApp="http://www.example.org/playApp">
  <playApp:App name="myApp" angularApp="/1">
    <backend>
      <resources name="Bodega" url="/api/Bodega/">
        <atributes xsi:type="playApp:PKAtribute" name="id" type="int"/>
        <atributes xsi:type="playApp:SimpleAtribute" name="nombre"/>
      </resources>
      <resources name="Producto" url="/api/Producto/">
        <atributes xsi:type="playApp:PKAtribute" name="id" type="int"/>
        <atributes xsi:type="playApp:SimpleAtribute" name="descripcion"/>
        <atributes xsi:type="playApp:SimpleAtribute" name="nombre"/>
      </resources>
      <resources name="SaldoBodega" url="/api/SaldoBodega/">
        <atributes xsi:type="playApp:PKAtribute" name="id" type="int"/>
        <atributes xsi:type="playApp:SimpleAtribute" name="cantidad" type="int"/>
        <atributes xsi:type="playApp:RefAtribute" name="idProducto" type="int" ref="/0/@backend/@resources.1"/>
        <atributes xsi:type="playApp:RefAtribute" name="idBodega" type="int" ref="/0/@backend/@resources.0"/>
      </resources>
    </backend>
  </playApp:App>
  <playApp:AngularApp>
    <pages route="/Bodega" resource="/0/@backend/@resources.0">
      <controller name="bodegaCtrl"/>
      <template/>
    </pages>
    <pages route="/Producto" resource="/0/@backend/@resources.1">
      <controller name="productoCtrl"/>
      <template/>
    </pages>
    <pages route="/SaldoBodega" resource="/0/@backend/@resources.2">
      <controller name="saldoBodegaCtrl"/>
      <template/>
    </pages>
  </playApp:AngularApp>
</xmi:XMI>
